def josephus(n: int, k: int) -> int:
    # Base case
    if n == 1:
        return 1
    return (josephus(n - 1, k) + k - 1) % n + 1
if __name__ == "__main__":
    print("Input: n = 3, k = 2")
    print("Output:", josephus(3, 2))  # Expected 3

    print("\nInput: n = 5, k = 3")
    print("Output:", josephus(5, 3))  # Expected 4
